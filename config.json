{
    "simulation": {
        "time_steps": 1000,
        "dt": 0.1,
        "random_seed": 42
    },
    "network": {
        "nodes": 10,
        "connections": 0.3,
        "bandwidth": 1000,
        "latency": 10,
        "reliability": 0.95,
        "encryption": true,
        "threat_level": 0.1,
        "load": 0.5
    },
    "adverse_conditions": {
        "noise_level": 0.1,
        "interference_probability": 0.05,
        "failure_rate": 0.02,
        "jamming_intensity": 0.1,
        "attack_probability": 0.01,
        "overload_threshold": 0.8,
        "degradation_factor": 0.2
    },
    "visualization": {
        "update_interval": 100,
        "graph_style": "default",
        "color_scheme": "viridis",
        "show_node_states": true,
        "show_degradation": true,
        "node_size_factor": 1.0,
        "edge_width_factor": 1.0
    },
    "analysis": {
        "metrics": ["throughput", "latency", "reliability", "availability", "connectivity", "data_loss"],
        "confidence_level": 0.95,
        "connectivity_analysis": true,
        "performance_degradation": true,
        "what_if_scenarios": 10
    },
    "node_types": {
        "server": {
            "capacity_range": [500, 2000],
            "reliability_range": [0.95, 0.99],
            "threat_level_range": [0.05, 0.15]
        },
        "router": {
            "capacity_range": [200, 1000],
            "reliability_range": [0.90, 0.98],
            "threat_level_range": [0.10, 0.20]
        },
        "switch": {
            "capacity_range": [100, 500],
            "reliability_range": [0.92, 0.97],
            "threat_level_range": [0.08, 0.18]
        },
        "client": {
            "capacity_range": [50, 200],
            "reliability_range": [0.85, 0.95],
            "threat_level_range": [0.15, 0.25]
        },
        "gateway": {
            "capacity_range": [300, 800],
            "reliability_range": [0.88, 0.96],
            "threat_level_range": [0.12, 0.22]
        },
        "firewall": {
            "capacity_range": [150, 600],
            "reliability_range": [0.90, 0.97],
            "threat_level_range": [0.05, 0.15]
        }
    },
    "link_types": {
        "ethernet": {
            "bandwidth_range": [10, 1000],
            "latency_range": [0.1, 2.0],
            "reliability_range": [0.95, 0.99]
        },
        "wifi": {
            "bandwidth_range": [1, 100],
            "latency_range": [1.0, 10.0],
            "reliability_range": [0.80, 0.95]
        },
        "fiber": {
            "bandwidth_range": [100, 10000],
            "latency_range": [0.1, 1.0],
            "reliability_range": [0.98, 0.999]
        },
        "copper": {
            "bandwidth_range": [1, 100],
            "latency_range": [1.0, 5.0],
            "reliability_range": [0.85, 0.98]
        },
        "wireless": {
            "bandwidth_range": [1, 50],
            "latency_range": [5.0, 50.0],
            "reliability_range": [0.70, 0.90]
        }
    }
}







